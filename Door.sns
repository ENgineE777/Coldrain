#import "Main"

class Door
{
	float x,y;	
	int visible = 1;
	int level;
}

class DoorController
{
	array<Door> door;
	Main@ main;

	void OnEnter(int self_index, string&in name, int index)
	{
		core.Log(name + " " + self_index);

		if (main.CanOpen(door[0].level))
		{
			door[0].visible = 0;
		}
	}

	void OnLeave(int self_index, string&in name, int index)
	{
		door[0].visible = 1;
	}
}